# Copy this file to .env and fill in API keys for the providers you use. Do not commit .env to git.
# In config.yaml, set provider per component (world_model_llm, rule_extractor_llm, action_agent_llm);
# each component then uses the corresponding provider's key/URL from here.

# OpenAI (provider: "openai")
OPENAI_API_KEY=
OPENAI_API_URL=

# Qwen / DashScope (provider: "qwen")
QWEN_API_KEY=
QWEN_API_URL=

# DeepSeek (provider: "deepseek")
DEEPSEEK_API_KEY=
DEEPSEEK_API_URL=

# Custom / OpenAI-compatible endpoint (provider: "custom")
CUSTOM_API_KEY=
CUSTOM_API_URL=
